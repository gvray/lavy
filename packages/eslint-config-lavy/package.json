{
	"name": "eslint-config-lavy",
	"version": "2.0.0",
	"description": "ESLint 9 flat config with TypeScript, React, Vue support",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./js": {
			"types": "./dist/js.d.ts",
			"import": "./dist/js.js",
			"require": "./dist/js.cjs"
		},
		"./ts": {
			"types": "./dist/ts.d.ts",
			"import": "./dist/ts.js",
			"require": "./dist/ts.cjs"
		},
		"./jsx": {
			"types": "./dist/jsx.d.ts",
			"import": "./dist/jsx.js",
			"require": "./dist/jsx.cjs"
		},
		"./tsx": {
			"types": "./dist/tsx.d.ts",
			"import": "./dist/tsx.js",
			"require": "./dist/tsx.cjs"
		},
		"./vue": {
			"types": "./dist/vue.d.ts",
			"import": "./dist/vue.js",
			"require": "./dist/vue.cjs"
		}
	},
	"files": [
		"dist"
	],
	"keywords": [
		"eslint",
		"eslintconfig",
		"flat-config",
		"lavy",
		"typescript",
		"react",
		"vue"
	],
	"type": "module",
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"clean": "rimraf dist",
		"prepublishOnly": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gvray/lavy.git"
	},
	"author": {
		"name": "Gavin Ray",
		"email": "gavinraydev@gmail.com"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/gvray/lavy/issues"
	},
	"homepage": "https://github.com/gvray/lavy#readme",
	"engines": {
		"node": ">=18"
	},
	"peerDependencies": {
		"eslint": "^9.0.0",
		"globals": "16.5.0"
	},
	"peerDependenciesMeta": {
		"@typescript-eslint/eslint-plugin": {
			"optional": true
		},
		"@typescript-eslint/parser": {
			"optional": true
		},
		"eslint-plugin-react": {
			"optional": true
		},
		"eslint-plugin-react-hooks": {
			"optional": true
		},
		"eslint-plugin-vue": {
			"optional": true
		}
	},
	"dependencies": {
		"@eslint/js": "^9.0.0"
	},
	"optionalDependencies": {
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"eslint-plugin-react": "^7.34.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-vue": "^9.20.0"
	},
	"devDependencies": {
		"eslint": "^9.0.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"eslint-plugin-react": "^7.34.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-vue": "^9.20.0",
		"typescript": "^5.0.0",
		"tsup": "^8.0.0"
	}
}