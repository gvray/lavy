{
	"name": "stylelint-config-lavy",
	"version": "1.0.0",
	"description": "Stylelint config with standard and prettier.",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist"
	],
	"type": "module",
	"keywords": [
		"stylelint",
		"stylelint-config",
		"config-standard",
		"prettier",
		"lavy"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"clean": "rimraf dist"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gvray/lavy.git"
	},
	"author": {
		"name": "Gavin Ray",
		"email": "gavinraydev@gmail.com"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/gvray/lavy/issues"
	},
	"homepage": "https://github.com/gvray/lavy#readme",
	"engines": {
		"node": ">=16"
	},
	"peerDependencies": {
		"stylelint": "14.x"
	},
	"dependencies": {
		"stylelint": "14.x",
		"stylelint-config-standard": "^29.0.0"
	}
}